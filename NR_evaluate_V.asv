init_epoch = 9.824283500767865e+08;
final_epoch = 9.824328897124767e+08;

init_state = [R0; V0+dV];
    

rkv_89emb = false;
ode_87 = true;



    %% RKV89
    if rkv_89emb 
    [t, y0state, output, stateE] = full_rkv89emb_maneuvers(@full_force_model, init_epoch, init_state);
    
    ystar = [y0state(4);y0state(6)];
